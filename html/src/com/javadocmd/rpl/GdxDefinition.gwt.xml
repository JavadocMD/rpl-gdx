<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
	<inherits name='com.badlogic.ashley_gwt' />
	<inherits name='com.badlogic.gdx.ai' />

	<inherits name='RplGame' />
	<entry-point class='com.javadocmd.rpl.client.HtmlLauncher' />
	
	<set-configuration-property name="gdx.assetpath" value="../core/assets" />
	
	<extend-configuration-property name="gdx.reflect.include" value="com.javadocmd.rpl.terminal.CommandEvent" />
	<extend-configuration-property name="gdx.reflect.include" value="com.javadocmd.rpl.terminal.NotificationEvent" />
</module>
